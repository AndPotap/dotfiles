alias vc='nvim /tmp/command.txt'
alias dot='cd ~/dotfiles'
alias rm='rm'
alias mv='mv -i'

alias pro='cd ~/cbetadir/src'
alias not='cd ~/notes && gp'
alias white='open ~/Pictures/White\ Noise.mp4'

alias tls='tmux ls'
alias tkill='tmux kill-session -t'
alias tas='tmux attach-session -t'
